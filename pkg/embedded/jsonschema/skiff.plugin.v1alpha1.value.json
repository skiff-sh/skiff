{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "$schema": {
      "type": "string"
    },
    "bool": {
      "type": "boolean"
    },
    "list": {
      "additionalProperties": false,
      "properties": {
        "numbers": {
          "items": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "enum": [
                  "Infinity",
                  "-Infinity",
                  "NaN"
                ],
                "type": "string"
              }
            ]
          },
          "type": "array"
        },
        "strings": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "title": "List",
      "type": "object"
    },
    "number": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "enum": [
            "Infinity",
            "-Infinity",
            "NaN"
          ],
          "type": "string"
        }
      ]
    },
    "string": {
      "type": "string"
    }
  },
  "title": "Value",
  "type": "object"
}
